<plugins:InteractivePluginBase x:Class="AloysAdjustments.Plugins.Misc.MiscControl"
                         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                         xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                         xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                         xmlns:local="clr-namespace:AloysAdjustments.Plugins.Misc"
                         xmlns:plugins="clr-namespace:AloysAdjustments.Plugins;assembly=AloysAdjustments"
                         mc:Ignorable="d" 
                         d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <CheckBox x:Name="cbIntroLogos" Content="Remove intro logos" Margin="10,10,0,0" 
                  HorizontalAlignment="Left" VerticalAlignment="Top" 
                  Checked="cbIntroLogos_Checked" Unchecked="cbIntroLogos_Checked"/>
        <CheckBox x:Name="cbMenuMusic" Content="Remove in-game menu music" Margin="10,30,0,0" 
                  HorizontalAlignment="Left" VerticalAlignment="Top" 
                  Checked="cbMenuMusic_Checked" Unchecked="cbMenuMusic_Checked"/>
    </Grid>
</plugins:InteractivePluginBase>
